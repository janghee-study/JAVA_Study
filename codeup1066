import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int i;
        int []arr = new int[3];
        for(i=0 ;i<3;i++){
        	arr[i] = sc.nextInt();
        }
        for(i=0;i<3;i++) {
        	if(arr[i]%2!=1) {
        		System.out.println("even");
        	}
        	else {
        		System.out.println("odd");
        	}
        }
	}
}
