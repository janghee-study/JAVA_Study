import java.util.*;

public class Main {
	public static int mod= 15746;
	public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        
        int n = sc.nextInt();
        int arr[]=new int[1000001];
        
        arr[1]=1;
        arr[2]=2;
        arr[3]=3;
        
        for(int i=3;i<=n;i++) {
        	arr[i]=arr[i-1]+arr[i-2];
        }
        System.out.println(arr[n]%mod);
	}
}#1904
