import java.io.*;
import java.util.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();
		int cnt = 0;
		int print = 0;
		
		for(int i=0;i<n;i++) {
			int input = sc.nextInt();
			cnt = 0;
			for(int j=1;j<=input;j++) {
				if(input%j==0) {
					cnt++;
				}
				
			}
			if(cnt==2) {
				print++;
			}
			
			
		}
		
		System.out.println(print);
	}
}#2581
