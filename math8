import java.util.*;
public class Main {
	public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int input = sc.nextInt();
        if(input==0) input=1;
        for(int i=input-1;i>1;i--) {
        	input*=i; 	
        }
        System.out.println(input);
    }
}
