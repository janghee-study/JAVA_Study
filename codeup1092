import java.util.Scanner;
import java.util.StringTokenizer;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        int day;
        for(day = 1 ;; day++ ){
        	if(day%a==0&&day%b==0&&day%c==0) {break;
        	}        	
        }
		System.out.println(day);
    }
}
