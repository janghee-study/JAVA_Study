import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        String aa = sc.nextLine();
        int num = Integer.parseInt(aa, 16); 
        for(int i=1; i<16; i++) {
        	System.out.format("%X*%X=%X%n",num,i,num*i);
        }     
    }
}
