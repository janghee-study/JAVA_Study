import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        int x ;
        x = ( a<b ? a:b );
        x = ( x<c ? x:c );
        System.out.println(x);
	}
}
