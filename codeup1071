import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int x = 100000;
		int []arr = new int[x];
		for(int i = 0; i <= x ;i++) {
			arr[i] = sc.nextInt();
				if(arr[i]==0)
					break;
				else {
					System.out.println(arr[i]);
		}
	}
	}
}
